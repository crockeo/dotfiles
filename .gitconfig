[core]
    pager = delta

[delta]
    navigate = true
    light = false

[diff]
    colorMoved = default

[interactive]
    diffFilter = delta --color-only

[merge]
    conflitstyle = diff3

[user]
    name = Cerek Hillen
    email = cerekh@gmail.com

[alias]
    tree = log --graph --decorate --pretty=oneline --abbrev-commit

[push]
    autoSetupRemote = true

[pull]
	rebase = true

[fetch]
	prune = true
